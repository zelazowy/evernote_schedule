<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="{{ asset("assets/vendor/Materialize/bin/materialize.css") }}">

    <!-- Compiled and minified JavaScript -->
    <script src="{{ asset("assets/vendor/Materialize/bin/materialize.js") }}"></script>

</head>
<body>

{#<header>#}
    {#<ul class="col s12 m4 l3 side-nav fixed">#}
        {#menu#}
    {#</ul>#}
{#</header>#}

<main>
        <div class="row">
            <div class="col s12 m12 l12">
                {% block body %}{% endblock %}
            </div>
        </div>
</main>

{% block javascripts %}{% endblock %}
</body>
</html>
